{
  "name": "@cistov/devtraining-seed",
  "description": "Devtraining seed",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "scripts": {},
  "devDependencies": {},
  "genxSeedConfig": {
    "persistAnswers": true,
    "processFiles": {
      "exclude": [
        ".git",
        ".genx",
        "node_modules",
        ".gradle",
        "gradle"
      ],
      "rename": true,
      "interpolateVariables": {
        "enabled": true,
        "include": [
          "Dockerfile",
          "product-info",
          ".yml",
          ".sh",
          ".json",
          ".js",
          ".kt",
          ".kts",
          ".ts",
          ".md",
          ".properties",
          ".html",
          ".xml"
        ]
      }
    }
  }
}
